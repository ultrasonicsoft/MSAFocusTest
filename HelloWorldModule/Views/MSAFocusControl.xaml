<UserControl x:Class="HelloWorldModule.Views.MSAFocusControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HelloWorldModule.Views"
             xmlns:viewModel="clr-namespace:HelloWorldModule.ViewModel"
             mc:Ignorable="d" 
              d:DataContext="{d:DesignInstance viewModel:MSAFocusControlViewModel, IsDesignTimeCreatable=True}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TextBlock Text="{Binding Id}"></TextBlock>
    </Grid>
</UserControl>
